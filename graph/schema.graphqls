# iRacelog GraphQL schema
#
#

type Event {
  id: ID!
  name: String!
  key: String!
  track: Track!
}

type User {
  id: ID!
  name: String!
}

type Track {
  id: ID!
  name: String!
  shortName: String!
  length: Float!
  events: [Event!]
}

type Query {
  getEvents: [Event!]!
  getTracks: [Track!]!
  track(id: ID): Track
}
